--
-- PS_ADM_APPL_RCR_CA_VW  (View) 
--
CREATE OR REPLACE VIEW CSSTG_OWNER.PS_ADM_APPL_RCR_CA_VW
BEQUEATH DEFINER
AS 
SELECT 
EMPLID, 
ACAD_CAREER, 
STDNT_CAR_NBR, 
ADM_APPL_NBR, 
INSTITUTION, 
RECRUITMENT_CAT, 
SRC_SYS_ID, 
RECRUIT_SUB_CAT, 
LOAD_ERROR, 
DATA_ORIGIN, 
CREATED_EW_DTTM, 
LASTUPD_EW_DTTM, 
BATCH_SID, 
CAST (TRIM (SUBSTR (DESCRLONG, 1, 2000)) AS VARCHAR2 (2000)) DESCRLONG
FROM PS_ADM_APPL_RCR_CA
/
