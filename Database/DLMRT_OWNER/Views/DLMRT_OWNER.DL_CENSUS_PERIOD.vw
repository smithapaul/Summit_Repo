DROP VIEW DLMRT_OWNER.DL_CENSUS_PERIOD
/

--
-- DL_CENSUS_PERIOD  (View) 
--
CREATE OR REPLACE VIEW DLMRT_OWNER.DL_CENSUS_PERIOD
BEQUEATH DEFINER
AS 
SELECT CENSUS_PERIOD,
           TERM_DESCRIPTION,
           CALENDAR_YEAR,
           ACADEMIC_YEAR_FWSPSU,
           ACADEMIC_YEAR_SUFWSP,
           FISCAL_YEAR,
           AID_YEAR,
           CIP_YEAR,
           INSERT_TIME
      FROM DLMRT_OWNER.CENSUS_PERIOD
/
