CREATE OR REPLACE VIEW C_UM_D_CRSE_ATTR_VAL_VW
BEQUEATH DEFINER
AS 
SELECT CAST(CRSE_ATTR_VAL_SID AS NUMBER(10))         CRSE_ATTR_VAL_SID,  
		  CRSE_CD, 
	      CRSE_OFFER_NUM, 
	      CRSE_ATTR, 
	      CRSE_ATTR_VALUE, 
	      SRC_SYS_ID, 
	      CRSE_SID, 
	      EFFDT, 
	      CRSE_ATTR_SD, 
	      CRSE_ATTR_LD, 
	      CRSE_ATTR_VALUE_LD, 
	      CRSE_ATTR_VALUE_FD, 
	      CATALOG_PRINT_FLG,
	      SCHEDULE_PRINT_FLG,
	      CAST('N' AS VARCHAR2(1))                 LOAD_ERROR,
		  DATA_ORIGIN,
		  CREATED_EW_DTTM,
		  LASTUPD_EW_DTTM,
		  CAST(1234 AS NUMBER(10))                 BATCH_SID
	 FROM CSMRT_OWNER.UM_D_CRSE_ATTR_VAL 
  WHERE CRSE_ATTR <> '-'
    AND DATA_ORIGIN <> 'D';
