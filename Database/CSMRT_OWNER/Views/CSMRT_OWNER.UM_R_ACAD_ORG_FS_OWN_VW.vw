--
-- UM_R_ACAD_ORG_FS_OWN_VW  (View) 
--
CREATE OR REPLACE VIEW CSMRT_OWNER.UM_R_ACAD_ORG_FS_OWN_VW
BEQUEATH DEFINER
AS 
SELECT 
    INSTITUTION_CD,
    ACAD_ORG_CD,
    DEPTID,
    EFFDT,
    SRC_SYS_ID,
    EFFDT_START,
    EFFDT_END,
    EFFDT_ORDER,
    INSTITUTION_SID,
    ACAD_ORG_SID,
    PERCENT_OWNED,
    DATA_ORIGIN,
    CREATED_EW_DTTM,
    LASTUPD_EW_DTTM
FROM PS_R_ACAD_ORG_FS_OWN
/
