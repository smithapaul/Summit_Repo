DROP VIEW CSMRT_OWNER.C_UM_D_STDNT_ATTR_VAL_VW
/

--
-- C_UM_D_STDNT_ATTR_VAL_VW  (View) 
--
CREATE OR REPLACE VIEW CSMRT_OWNER.C_UM_D_STDNT_ATTR_VAL_VW
BEQUEATH DEFINER
AS 
SELECT CAST(ROWNUM AS NUMBER(10))      STDNT_ATTR_VAL_SID,  
		PERSON_ID                       EMPLID, 
		ACAD_CAR_CD, 
		STDNT_CAR_NUM, 
		STDNT_ATTR, 
		STDNT_ATTR_VALUE, 
		SRC_SYS_ID, 
		INSTITUTION_CD, 
		PERSON_SID, 
		INSTITUTION_SID, 
		ACAD_CAR_SID, 
		EFFDT, 
		EFFSEQ, 
		STDNT_ATTR_SD, 
		STDNT_ATTR_LD, 
		STDNT_ATTR_VAL_SD, 
		STDNT_ATTR_VAL_LD, 
		ATTR_ORDER, 
		CAST(UMBOS_UGRD_SECOND_DEGR_FLG AS CHAR(1)) UMBOS_UGRD_SECOND_DEGR_FLG,
	    CAST('N' AS VARCHAR2(1))                 LOAD_ERROR,
		DATA_ORIGIN,
		CREATED_EW_DTTM,
		LASTUPD_EW_DTTM,
		CAST(1234 AS NUMBER(10))                 BATCH_SID
   FROM CSMRT_OWNER.UM_D_STDNT_ATTR_VAL 
  WHERE DATA_ORIGIN <> 'D'
/
