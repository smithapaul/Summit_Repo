CREATE OR REPLACE VIEW UM_F_SAA_ADB_TERM_VW
BEQUEATH DEFINER
AS 
select /*+ OPT_ESTIMATE(QUERY_BLOCK MIN=100000) */ 
       INSTITUTION_CD,
       MIN_TERM_CD,
       MAX_TERM_CD
  from (
     SELECT T.INSTITUTION_CD,
            MIN (T.TERM_CD) MIN_TERM_CD,
            MAX (T.TERM_CD) MAX_TERM_CD
       FROM UM_F_SAA_ADB_RESULTS R, PS_D_TERM T
      WHERE R.TERM_SID = T.TERM_SID
   GROUP BY T.INSTITUTION_CD)
 where ROWNUM < 100000000;
