CREATE OR REPLACE VIEW C_UM_D_PROG_ACN_VW
BEQUEATH DEFINER
AS 
SELECT CAST(PROG_ACN_SID AS NUMBER(10))      PROG_ACN_SID,  
		CAST(SETID AS CHAR(5))                SETID, 
		PROG_ACN_CD, 
		SRC_SYS_ID, 
		PROG_ACN_SD, 
		PROG_ACN_LD, 
		CAST(PROG_ACN_CD_DESC AS VARCHAR2(37))  PROG_ACN_CD_DESC
   FROM CSMRT_OWNER.PS_D_PROG_ACN
  WHERE DATA_ORIGIN <> 'D';
