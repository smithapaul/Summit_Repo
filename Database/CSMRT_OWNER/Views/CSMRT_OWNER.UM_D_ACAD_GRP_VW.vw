DROP VIEW CSMRT_OWNER.UM_D_ACAD_GRP_VW
/

--
-- UM_D_ACAD_GRP_VW  (View) 
--
CREATE OR REPLACE VIEW CSMRT_OWNER.UM_D_ACAD_GRP_VW
BEQUEATH DEFINER
AS 
select ACAD_GRP_SID, EFFDT, INSTITUTION_CD, ACAD_GRP_CD, SRC_SYS_ID, 
       EFFDT_START, EFFDT_END, EFFDT_ORDER, EFF_STAT_CD, ACAD_GRP_SD, ACAD_GRP_LD, ACAD_GRP_CD_DESC,
       DATA_ORIGIN, CREATED_EW_DTTM, LASTUPD_EW_DTTM
  from PS_D_ACAD_GRP
 where EFFDT_ORDER = 1		-- Remove when PRD has been modified for effective dating.
/
