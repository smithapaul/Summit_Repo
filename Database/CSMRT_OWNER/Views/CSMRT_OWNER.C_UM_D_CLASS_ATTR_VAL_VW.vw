CREATE OR REPLACE VIEW C_UM_D_CLASS_ATTR_VAL_VW
BEQUEATH DEFINER
AS 
SELECT CAST(CLASS_ATTR_VAL_SID AS NUMBER(10))         CLASS_ATTR_VAL_SID,  
		CRSE_CD, 
		CRSE_OFFER_NUM, 
		TERM_CD, 
		SESSION_CD, 
		CLASS_SECTION_CD, 
		CRSE_ATTR, 
		CRSE_ATTR_VALUE, 
		SRC_SYS_ID, 
		CLASS_SID, 
		NVL(CRSE_ATTR_SD, '-')                  CRSE_ATTR_SD,
		NVL(CRSE_ATTR_LD, '-')                  CRSE_ATTR_LD,
		NVL(CRSE_ATTR_VALUE_LD, '-')            CRSE_ATTR_VALUE_LD,
		NVL(CRSE_ATTR_VALUE_FD, '-')            CRSE_ATTR_VALUE_FD,
		NVL(CATALOG_PRINT_FLG, '-')             CATALOG_PRINT_FLG,
		NVL(SCHEDULE_PRINT_FLG, '-')            SCHEDULE_PRINT_FLG,
	    CAST('N' AS VARCHAR2(1))                LOAD_ERROR,
		DATA_ORIGIN,
		CREATED_EW_DTTM,
		LASTUPD_EW_DTTM,
		CAST(1234 AS NUMBER(10))                 BATCH_SID
   FROM CSMRT_OWNER.UM_D_CLASS_ATTR_VAL 
  WHERE CRSE_ATTR <> '-'
    AND DATA_ORIGIN <> 'D';
