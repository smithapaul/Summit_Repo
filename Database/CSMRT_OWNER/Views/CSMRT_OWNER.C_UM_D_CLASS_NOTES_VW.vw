CREATE OR REPLACE VIEW C_UM_D_CLASS_NOTES_VW
BEQUEATH DEFINER
AS 
SELECT CRSE_CD, 
		   CRSE_OFFER_NUM, 
		   TERM_CD, 
		   SESSION_CD, 
		   CLASS_SECTION_CD, 
		   CLASS_NOTES_SEQ, 
		   SRC_SYS_ID, 
		   CAST(CLASS_SID AS NUMBER(10)) CLASS_SID, 
		   PRINT_AT, 
		   CLASS_NOTE_NBR, 
		   PRINT_NOTE_W_O_CLS,
           DATA_ORIGIN,
           CREATED_EW_DTTM,
           LASTUPD_EW_DTTM,
		   DESCRLONG,
		   INSTITUTION_CD
      FROM csmrt_owner.UM_D_CLASS_NOTES
	 WHERE DATA_ORIGIN <> 'D';
