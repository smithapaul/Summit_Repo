CREATE OR REPLACE VIEW C_PS_STDNT_GRPS_HIST_VW
BEQUEATH DEFINER
AS 
SELECT EMPLID, 
		   INSTITUTION, 
		   STDNT_GROUP, 
		   EFFDT, 
		   SRC_SYS_ID, 
		   EFF_STATUS, 
		   LOAD_ERROR, 
		   DATA_ORIGIN, 
		   CREATED_EW_DTTM, 
		   LASTUPD_EW_DTTM, 
		   BATCH_SID, 
		   COMMENTS
	  FROM CSSTG_OWNER.PS_STDNT_GRPS_HIST
	 WHERE DATA_ORIGIN <> 'D';
