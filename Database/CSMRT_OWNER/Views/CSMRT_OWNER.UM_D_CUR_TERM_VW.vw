CREATE OR REPLACE VIEW UM_D_CUR_TERM_VW
BEQUEATH DEFINER
AS 
SELECT INSTITUTION_CD, ACAD_CAR_CD, CURRENT_TERM, SRC_SYS_ID, 
           PREVIOUS_WINTER_TERM, PREVIOUS_FALL_TERM, PREVIOUS_SPRING_TERM, PREVIOUS_SUMMER_TERM, PREVIOUS_TRI_TERM, PREVIOUS_TERM, 
           NEXT_TERM, NEXT_WINTER_TERM, NEXT_FALL_TERM, NEXT_SPRING_TERM, NEXT_SUMMER_TERM, NEXT_TRI_TERM, 
           DATA_ORIGIN, CREATED_EW_DTTM, LASTUPD_EW_DTTM
      FROM PS_D_CUR_TERM;
