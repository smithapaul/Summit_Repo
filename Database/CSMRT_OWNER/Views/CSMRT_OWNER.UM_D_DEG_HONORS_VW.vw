DROP VIEW CSMRT_OWNER.UM_D_DEG_HONORS_VW
/

--
-- UM_D_DEG_HONORS_VW  (View) 
--
CREATE OR REPLACE VIEW CSMRT_OWNER.UM_D_DEG_HONORS_VW
BEQUEATH DEFINER
AS 
select DEG_HONORS_SID, INSTITUTION_CD, HONORS_TYPE_CD, HONORS_CD, SRC_SYS_ID, EFFDT, EFF_STAT_CD, 
       HONORS_TYPE_SD, HONORS_TYPE_LD, HONORS_SD, HONORS_LD, HONORS_FD,
       DATA_ORIGIN, CREATED_EW_DTTM, LASTUPD_EW_DTTM
  from CSMRT_OWNER.PS_D_DEG_HONORS
/
