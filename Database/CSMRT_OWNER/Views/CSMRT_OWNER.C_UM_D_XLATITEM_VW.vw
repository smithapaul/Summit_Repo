DROP VIEW CSMRT_OWNER.C_UM_D_XLATITEM_VW
/

--
-- C_UM_D_XLATITEM_VW  (View) 
--
CREATE OR REPLACE VIEW CSMRT_OWNER.C_UM_D_XLATITEM_VW
BEQUEATH DEFINER
AS 
SELECT FIELDNAME, 
		   FIELDVALUE, 
		   SRC_SYS_ID, 
		   XLATLONGNAME, 
		   XLATSHORTNAME
	 FROM  CSMRT_OWNER.UM_D_XLATITEM
	WHERE DATA_ORIGIN <> 'D'
/
