CREATE OR REPLACE PROCEDURE             "PS_D_VAR_DATA_P" AUTHID CURRENT_USER IS

------------------------------------------------------------------------
-- George Adams
--
-- Loads stage table PS_D_VAR_DATA from PeopleSoft table PS_D_VAR_DATA.
--
--V01   SMT-xxxx 10/27/2017,    James Doucette
--                              Converted from DataStage
--V02   SMT-xxxx 03/15/2018,    James Doucette
--                              Updated INSTITUTION field to INSTITUTION_CD.
--
------------------------------------------------------------------------

        strMartId                       Varchar2(50)    := 'CSW';
        strProcessName                  Varchar2(100)   := 'PS_D_VAR_DATA';
        intProcessSid                   Integer;
        dtProcessStart                  Date            := SYSDATE;
        strMessage01                    Varchar2(4000);
        strMessage02                    Varchar2(512);
        strMessage03                    Varchar2(512)   :='';
        strNewLine                      Varchar2(2)     := chr(13) || chr(10);
        strSqlCommand                   Varchar2(32767) :='';
        strSqlDynamic                   Varchar2(32767) :='';
        strClientInfo                   Varchar2(100);
        intRowCount                     Integer;
        intTotalRowCount                Integer         := 0;
        numSqlCode                      Number;
        strSqlErrm                      Varchar2(4000);
        intTries                        Integer;

BEGIN

strSqlCommand := 'DBMS_APPLICATION_INFO.SET_CLIENT_INFO';
DBMS_APPLICATION_INFO.SET_CLIENT_INFO (strProcessName);

strSqlCommand := 'SMT_PROCESS_LOG.PROCESS_INIT';
COMMON_OWNER.SMT_PROCESS_LOG.PROCESS_INIT
        (
                i_MartId                => strMartId,
                i_ProcessName           => strProcessName,
                i_ProcessStartTime      => dtProcessStart,
                o_ProcessSid            => intProcessSid
        );

strMessage01    := 'Merging data into CSMRT_OWNER.PS_D_VAR_DATA';
COMMON_OWNER.SMT_LOG.PUT_MESSAGE(i_Message => strMessage01);

strSqlCommand   := 'merge into CSMRT_OWNER.PS_D_VAR_DATA';
merge /*+ use_hash(S,T) */ into CSMRT_OWNER.PS_D_VAR_DATA T                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
using (                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
  with Q1 as (  
select /*+ inline */ 
       COMMON_ID, VAR_DATA_SEQ, 'ADMP' ADMIN_FUNCTION, SRC_SYS_ID, 
       ACAD_CAREER ACAD_CAR_CD, STDNT_CAR_NBR, ADM_APPL_NBR, APPL_PROG_NBR, '-' AID_YEAR,
       '-' SESSION_NO, '-' GIFT_NO, '-' RECOGNITION_TYPE, '-' DESIGNATION, '-' INTV_CD, 
       '-' BUSINESS_UNIT, '-' STRM, '-' CAMPUS_EVENT_NBR, 0 EVENT_MTG_NBR, 
       '-' LOAN_TYPE, 0 LN_APPL_SEQ, '-' LN_APPL_ID, '-' ACCOUNT_NBR, '-' ACCOUNT_TERM, 
       DATA_ORIGIN
  from CSSTG_OWNER.PS_VAR_DATA_ADMP
 union all
select COMMON_ID, VAR_DATA_SEQ, 'AVAK' ADMIN_FUNCTION, SRC_SYS_ID, 
       '-' ACAD_CAR_CD, 0 STDNT_CAR_NBR, '-' ADM_APPL_NBR, 0 APPL_PROG_NBR, '-' AID_YEAR,
       SESSION_NO, GIFT_NO, RECOGNITION_TYPE, DESIGNATION, INTV_CD, 
       BUSINESS_UNIT, '-' STRM, '-' CAMPUS_EVENT_NBR, 0 EVENT_MTG_NBR, 
       '-' LOAN_TYPE, 0 LN_APPL_SEQ, '-' LN_APPL_ID, '-' ACCOUNT_NBR, '-' ACCOUNT_TERM, 
       DATA_ORIGIN
  from CSSTG_OWNER.PS_VAR_DATA_AVAK
 union all
select COMMON_ID, VAR_DATA_SEQ, 'EVNT' ADMIN_FUNCTION, SRC_SYS_ID, 
       '-' ACAD_CAR_CD, 0 STDNT_CAR_NBR, '-' ADM_APPL_NBR, 0 APPL_PROG_NBR, '-' AID_YEAR,
       '-' SESSION_NO, '-' GIFT_NO, '-' RECOGNITION_TYPE, '-' DESIGNATION, '-' INTV_CD, 
       '-' BUSINESS_UNIT, '-' STRM, CAMPUS_EVENT_NBR, EVENT_MTG_NBR, 
       '-' LOAN_TYPE, 0 LN_APPL_SEQ, '-' LN_APPL_ID, '-' ACCOUNT_NBR, '-' ACCOUNT_TERM, 
       DATA_ORIGIN
  from CSSTG_OWNER.PS_VAR_DATA_EVNT
 union all
select COMMON_ID, VAR_DATA_SEQ, 'FINA' ADMIN_FUNCTION, SRC_SYS_ID, 
       '-' ACAD_CAR_CD, 0 STDNT_CAR_NBR, '-' ADM_APPL_NBR, 0 APPL_PROG_NBR, AID_YEAR,
       '-' SESSION_NO, '-' GIFT_NO, '-' RECOGNITION_TYPE, '-' DESIGNATION, '-' INTV_CD, 
       '-' BUSINESS_UNIT, '-' STRM, '-' CAMPUS_EVENT_NBR, 0 EVENT_MTG_NBR, 
       '-' LOAN_TYPE, 0 LN_APPL_SEQ, '-' LN_APPL_ID, '-' ACCOUNT_NBR, '-' ACCOUNT_TERM, 
       DATA_ORIGIN
  from CSSTG_OWNER.PS_VAR_DATA_FINA
 union all
select COMMON_ID, VAR_DATA_SEQ, 'LOAN' ADMIN_FUNCTION, SRC_SYS_ID, 
       ACAD_CAREER ACAD_CAR_CD, 0 STDNT_CAR_NBR, '-' ADM_APPL_NBR, 0 APPL_PROG_NBR, AID_YEAR,
       '-' SESSION_NO, '-' GIFT_NO, '-' RECOGNITION_TYPE, '-' DESIGNATION, '-' INTV_CD, 
       '-' BUSINESS_UNIT, '-' STRM, '-' CAMPUS_EVENT_NBR, 0 EVENT_MTG_NBR, 
       LOAN_TYPE, LN_APPL_SEQ, LN_APPL_ID, '-' ACCOUNT_NBR, '-' ACCOUNT_TERM, 
       DATA_ORIGIN
  from CSSTG_OWNER.PS_VAR_DATA_LOAN
 union all
select COMMON_ID, VAR_DATA_SEQ, 'PROS' ADMIN_FUNCTION, SRC_SYS_ID, 
       ACAD_CAREER ACAD_CAR_CD, 0 STDNT_CAR_NBR, '-' ADM_APPL_NBR, 0 APPL_PROG_NBR, '-' AID_YEAR,
       '-' SESSION_NO, '-' GIFT_NO, '-' RECOGNITION_TYPE, '-' DESIGNATION, '-' INTV_CD, 
       '-' BUSINESS_UNIT, '-' STRM, '-' CAMPUS_EVENT_NBR, 0 EVENT_MTG_NBR, 
       '-' LOAN_TYPE, 0 LN_APPL_SEQ, '-' LN_APPL_ID, '-' ACCOUNT_NBR, '-' ACCOUNT_TERM, 
       DATA_ORIGIN
  from CSSTG_OWNER.PS_VAR_DATA_PROS
 union all
select COMMON_ID, VAR_DATA_SEQ, 'SFAC' ADMIN_FUNCTION, SRC_SYS_ID, 
       '-' ACAD_CAR_CD, 0 STDNT_CAR_NBR, '-' ADM_APPL_NBR, 0 APPL_PROG_NBR, '-' AID_YEAR,
       '-' SESSION_NO, '-' GIFT_NO, '-' RECOGNITION_TYPE, '-' DESIGNATION, '-' INTV_CD, 
       BUSINESS_UNIT, '-' STRM, '-' CAMPUS_EVENT_NBR, 0 EVENT_MTG_NBR, 
       '-' LOAN_TYPE, 0 LN_APPL_SEQ, '-' LN_APPL_ID, ACCOUNT_NBR, ACCOUNT_TERM, 
       DATA_ORIGIN
  from CSSTG_OWNER.PS_VAR_DATA_SFAC
 union all
select COMMON_ID, VAR_DATA_SEQ, 'STRM' ADMIN_FUNCTION, SRC_SYS_ID, 
       ACAD_CAREER ACAD_CAR_CD, 0 STDNT_CAR_NBR, '-' ADM_APPL_NBR, 0 APPL_PROG_NBR, '-' AID_YEAR,
       '-' SESSION_NO, '-' GIFT_NO, '-' RECOGNITION_TYPE, '-' DESIGNATION, '-' INTV_CD, 
       '-' BUSINESS_UNIT, STRM, '-' CAMPUS_EVENT_NBR, 0 EVENT_MTG_NBR, 
       '-' LOAN_TYPE, 0 LN_APPL_SEQ, '-' LN_APPL_ID, '-' ACCOUNT_NBR, '-' ACCOUNT_TERM, 
       DATA_ORIGIN
  from CSSTG_OWNER.PS_VAR_DATA_STRM),
       S as (
select /*+ inline */ 
       Q1.COMMON_ID, Q1.VAR_DATA_SEQ, Q1.ADMIN_FUNCTION, Q1.SRC_SYS_ID, 
       nvl(P.PERSON_SID,2147483646) PERSON_SID,   
       Q1.ACAD_CAR_CD, Q1.ACCOUNT_NBR, Q1.ACCOUNT_TERM, Q1.ADM_APPL_NBR, Q1.AID_YEAR, Q1.APPL_PROG_NBR, 
       Q1.BUSINESS_UNIT, Q1.CAMPUS_EVENT_NBR, Q1.DESIGNATION, Q1.EVENT_MTG_NBR, Q1.GIFT_NO, Q1.INTV_CD, 
       Q1.LN_APPL_ID, Q1.LN_APPL_SEQ, Q1.LOAN_TYPE, Q1.RECOGNITION_TYPE, Q1.SESSION_NO, Q1.STDNT_CAR_NBR, Q1.STRM, 
       Q1.DATA_ORIGIN  
  from Q1
  left outer join PS_D_PERSON P 
    on Q1.COMMON_ID = P.PERSON_ID
   and Q1.SRC_SYS_ID = P.SRC_SYS_ID
   and P.DATA_ORIGIN <> 'D')  
--select nvl(D.VAR_DATA_SID, max(D.VAR_DATA_SID) over (partition by 1) +                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
--       row_number() over (partition by 1 order by D.VAR_DATA_SID nulls first)) VAR_DATA_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
select nvl(D.VAR_DATA_SID, 
          (select nvl(max(VAR_DATA_SID),0) from CSMRT_OWNER.PS_D_VAR_DATA where VAR_DATA_SID < 2147483646) + 
                  row_number() over (partition by 1 order by D.VAR_DATA_SID nulls first)) VAR_DATA_SID,         -- Mar 2020 
       nvl(D.COMMON_ID, S.COMMON_ID) COMMON_ID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
       nvl(D.VAR_DATA_SEQ, S.VAR_DATA_SEQ) VAR_DATA_SEQ,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
       nvl(D.ADMIN_FUNCTION, S.ADMIN_FUNCTION) ADMIN_FUNCTION,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 
       nvl(D.SRC_SYS_ID, S.SRC_SYS_ID) SRC_SYS_ID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
       decode(D.PERSON_SID, S.PERSON_SID, D.PERSON_SID, S.PERSON_SID) PERSON_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
       decode(D.ACAD_CAR_CD, S.ACAD_CAR_CD, D.ACAD_CAR_CD, S.ACAD_CAR_CD) ACAD_CAR_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(D.ACCOUNT_NBR, S.ACCOUNT_NBR, D.ACCOUNT_NBR, S.ACCOUNT_NBR) ACCOUNT_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(D.ACCOUNT_TERM, S.ACCOUNT_TERM, D.ACCOUNT_TERM, S.ACCOUNT_TERM) ACCOUNT_TERM,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       decode(D.ADM_APPL_NBR, S.ADM_APPL_NBR, D.ADM_APPL_NBR, S.ADM_APPL_NBR) ADM_APPL_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       decode(D.AID_YEAR, S.AID_YEAR, D.AID_YEAR, S.AID_YEAR) AID_YEAR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       decode(D.APPL_PROG_NBR, S.APPL_PROG_NBR, D.APPL_PROG_NBR, S.APPL_PROG_NBR) APPL_PROG_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
       decode(D.BUSINESS_UNIT, S.BUSINESS_UNIT, D.BUSINESS_UNIT, S.BUSINESS_UNIT) BUSINESS_UNIT,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
       decode(D.CAMPUS_EVENT_NBR, S.CAMPUS_EVENT_NBR, D.CAMPUS_EVENT_NBR, S.CAMPUS_EVENT_NBR) CAMPUS_EVENT_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(D.DESIGNATION, S.DESIGNATION, D.DESIGNATION, S.DESIGNATION) DESIGNATION,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(D.EVENT_MTG_NBR, S.EVENT_MTG_NBR, D.EVENT_MTG_NBR, S.EVENT_MTG_NBR) EVENT_MTG_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
       decode(D.GIFT_NO, S.GIFT_NO, D.GIFT_NO, S.GIFT_NO) GIFT_NO,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
       decode(D.INTV_CD, S.INTV_CD, D.INTV_CD, S.INTV_CD) INTV_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
       decode(D.LN_APPL_ID, S.LN_APPL_ID, D.LN_APPL_ID, S.LN_APPL_ID) LN_APPL_ID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
       decode(D.LN_APPL_SEQ, S.LN_APPL_SEQ, D.LN_APPL_SEQ, S.LN_APPL_SEQ) LN_APPL_SEQ,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(D.LOAN_TYPE, S.LOAN_TYPE, D.LOAN_TYPE, S.LOAN_TYPE) LOAN_TYPE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
       decode(D.RECOGNITION_TYPE, S.RECOGNITION_TYPE, D.RECOGNITION_TYPE, S.RECOGNITION_TYPE) RECOGNITION_TYPE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(D.SESSION_NO, S.SESSION_NO, D.SESSION_NO, S.SESSION_NO) SESSION_NO,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
       decode(D.STDNT_CAR_NBR, S.STDNT_CAR_NBR, D.STDNT_CAR_NBR, S.STDNT_CAR_NBR) STDNT_CAR_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
       decode(D.STRM, S.STRM, D.STRM, S.STRM) STRM,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
       decode(D.DATA_ORIGIN, S.DATA_ORIGIN, D.DATA_ORIGIN, S.DATA_ORIGIN) DATA_ORIGIN,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       nvl(D.CREATED_EW_DTTM, SYSDATE) CREATED_EW_DTTM,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       nvl(D.LASTUPD_EW_DTTM, SYSDATE) LASTUPD_EW_DTTM                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
  from S                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
  left outer join CSMRT_OWNER.PS_D_VAR_DATA D                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
    on D.VAR_DATA_SID <> 2147483646                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
   and D.COMMON_ID = S.COMMON_ID                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
   and D.VAR_DATA_SEQ = S.VAR_DATA_SEQ                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
   and D.ADMIN_FUNCTION = S.ADMIN_FUNCTION                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
   and D.SRC_SYS_ID = S.SRC_SYS_ID                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
) S                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
    on  (T.COMMON_ID = S.COMMON_ID                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
   and  T.VAR_DATA_SEQ = S.VAR_DATA_SEQ                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
   and  T.ADMIN_FUNCTION = S.ADMIN_FUNCTION                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
   and  T.SRC_SYS_ID = S.SRC_SYS_ID)                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
 when matched then update set                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       T.PERSON_SID = S.PERSON_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
       T.ACAD_CAR_CD = S.ACAD_CAR_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.ACCOUNT_NBR = S.ACCOUNT_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.ACCOUNT_TERM = S.ACCOUNT_TERM,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       T.ADM_APPL_NBR = S.ADM_APPL_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       T.AID_YEAR = S.AID_YEAR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       T.APPL_PROG_NBR = S.APPL_PROG_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       T.BUSINESS_UNIT = S.BUSINESS_UNIT,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       T.CAMPUS_EVENT_NBR = S.CAMPUS_EVENT_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       T.DESIGNATION = S.DESIGNATION,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.EVENT_MTG_NBR = S.EVENT_MTG_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       T.GIFT_NO = S.GIFT_NO,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       T.INTV_CD = S.INTV_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       T.LN_APPL_ID = S.LN_APPL_ID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
       T.LN_APPL_SEQ = S.LN_APPL_SEQ,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.LOAN_TYPE = S.LOAN_TYPE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
       T.RECOGNITION_TYPE = S.RECOGNITION_TYPE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       T.SESSION_NO = S.SESSION_NO,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
       T.STDNT_CAR_NBR = S.STDNT_CAR_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       T.STRM = S.STRM,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       T.DATA_ORIGIN = S.DATA_ORIGIN,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.LASTUPD_EW_DTTM = SYSDATE                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
 where                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(T.PERSON_SID,S.PERSON_SID,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
       decode(T.ACAD_CAR_CD,S.ACAD_CAR_CD,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(T.ACCOUNT_NBR,S.ACCOUNT_NBR,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(T.ACCOUNT_TERM,S.ACCOUNT_TERM,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       decode(T.ADM_APPL_NBR,S.ADM_APPL_NBR,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       decode(T.AID_YEAR,S.AID_YEAR,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(T.APPL_PROG_NBR,S.APPL_PROG_NBR,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(T.BUSINESS_UNIT,S.BUSINESS_UNIT,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(T.CAMPUS_EVENT_NBR,S.CAMPUS_EVENT_NBR,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(T.DESIGNATION,S.DESIGNATION,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(T.EVENT_MTG_NBR,S.EVENT_MTG_NBR,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(T.GIFT_NO,S.GIFT_NO,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       decode(T.INTV_CD,S.INTV_CD,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       decode(T.LN_APPL_ID,S.LN_APPL_ID,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
       decode(T.LN_APPL_SEQ,S.LN_APPL_SEQ,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(T.LOAN_TYPE,S.LOAN_TYPE,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
       decode(T.RECOGNITION_TYPE,S.RECOGNITION_TYPE,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(T.SESSION_NO,S.SESSION_NO,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
       decode(T.STDNT_CAR_NBR,S.STDNT_CAR_NBR,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(T.STRM,S.STRM,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       decode(T.DATA_ORIGIN,S.DATA_ORIGIN,0,1) = 1                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
  when not matched then                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
insert (                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
       T.VAR_DATA_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       T.COMMON_ID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
       T.VAR_DATA_SEQ,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       T.ADMIN_FUNCTION,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
       T.SRC_SYS_ID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
       T.PERSON_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
       T.ACAD_CAR_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.ACCOUNT_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.ACCOUNT_TERM,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       T.ADM_APPL_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       T.AID_YEAR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
