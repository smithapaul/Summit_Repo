       T.MIN_CRSES_REQD = S.MIN_CRSES_REQD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       T.RQRMNT_USEAGE = S.RQRMNT_USEAGE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
