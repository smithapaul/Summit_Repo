       decode(T.RQRMNT_USEAGE_SD,S.RQRMNT_USEAGE_SD,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(T.RQRMNT_USEAGE_LD,S.RQRMNT_USEAGE_LD,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(T.RQRMNT_LIST_SEQ,S.RQRMNT_LIST_SEQ,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       decode(T.RQ_CONNECT_TYPE,S.RQ_CONNECT_TYPE,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       decode(T.SAA_HIDE_STATUS,S.SAA_HIDE_STATUS,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
