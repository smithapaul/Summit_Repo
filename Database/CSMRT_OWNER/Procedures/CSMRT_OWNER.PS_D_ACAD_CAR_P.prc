strSqlCommand   := 'update DATA_ORIGIN on CSMRT_OWNER.PS_D_ACAD_CAR';

update CSMRT_OWNER.PS_D_ACAD_CAR T 
   set DATA_ORIGIN = 'D',
       LASTUPD_EW_DTTM = SYSDATE
 where DATA_ORIGIN <> 'D'
   and T.ACAD_CAR_SID < 2147483646
   and not exists (select 1
                     from CSSTG_OWNER.PS_ACAD_CAR_TBL S
                    where T.INSTITUTION_CD = S.INSTITUTION
                      and T.ACAD_CAR_CD = S.ACAD_CAREER
                      and T.SRC_SYS_ID = S.SRC_SYS_ID
					  and S.DATA_ORIGIN <> 'D');

