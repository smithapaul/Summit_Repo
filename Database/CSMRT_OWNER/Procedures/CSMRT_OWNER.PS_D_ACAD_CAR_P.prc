CREATE OR REPLACE PROCEDURE             "PS_D_ACAD_CAR_P" AUTHID CURRENT_USER IS

------------------------------------------------------------------------
--George Adams
--OLD tables               -- PS_D_ACAD_CAR/PS_D_ACAD_CAR_VW
--Loads target table       -- PS_D_ACAD_CAR
-- V01 4/17/2018           -- srikanth ,pabbu converted to proc from sql
-- V02 2/11/2021            -- Srikanth,Pabbu made changes to ACAD_CAR_SID field
------------------------------------------------------------------------

        strMartId                       Varchar2(50)    := 'CSW';
        strProcessName                  Varchar2(100)   := 'PS_D_ACAD_CAR';
        intProcessSid                   Integer;
        dtProcessStart                  Date            := SYSDATE;
        strMessage01                    Varchar2(4000);
        strMessage02                    Varchar2(512);
        strMessage03                    Varchar2(512)   :='';
        strNewLine                      Varchar2(2)     := chr(13) || chr(10);
        strSqlCommand                   Varchar2(32767) :='';
        strSqlDynamic                   Varchar2(32767) :='';
        strClientInfo                   Varchar2(100);
        intRowCount                     Integer;
        intTotalRowCount                Integer         := 0;
        numSqlCode                      Number;
        strSqlErrm                      Varchar2(4000);
        intTries                        Integer;

BEGIN

strSqlCommand := 'DBMS_APPLICATION_INFO.SET_CLIENT_INFO';
DBMS_APPLICATION_INFO.SET_CLIENT_INFO (strProcessName);

strSqlCommand := 'SMT_PROCESS_LOG.PROCESS_INIT';
COMMON_OWNER.SMT_PROCESS_LOG.PROCESS_INIT
        (
                i_MartId                => strMartId,
                i_ProcessName           => strProcessName,
                i_ProcessStartTime      => dtProcessStart,
                o_ProcessSid            => intProcessSid
        );

strMessage01    := 'Merging data into CSMRT_OWNER.PS_D_ACAD_CAR';
COMMON_OWNER.SMT_LOG.PUT_MESSAGE(i_Message => strMessage01);

strSqlCommand   := 'merge into CSMRT_OWNER.PS_D_ACAD_CAR';

merge /*+ use_hash(S,T) */ into CSMRT_OWNER.PS_D_ACAD_CAR T                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
using (                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
with Q1 as (  
select INSTITUTION INSTITUTION_CD, ACAD_CAREER ACAD_CAR_CD, SRC_SYS_ID, EFFDT, EFF_STATUS EFF_STAT_CD, 
       DESCR ACAD_CAR_LD, DESCRSHORT ACAD_CAR_SD, GRADING_SCHEME, GRADING_BASIS, GRADE_TRANSFER, REPEAT_SCHEME,  
       FA_PRIMACY_NBR, ACAD_PLAN_TYPE, SF_GRAD_DESIGNATIO, FA_CAR_TYPE, GRADUATE_LVL_IND, 
       OEE_IND, REPEAT_RULE, REPEAT_ENRL_CTL, REPEAT_ENRL_SUSP, REPEAT_GRD_CK, REPEAT_GRD_SUSP, REPEAT_CRSE_ERROR, 
       DATA_ORIGIN,  
       row_number() over (partition by INSTITUTION, ACAD_CAREER, SRC_SYS_ID
                              order by DATA_ORIGIN desc, (case when EFFDT > trunc(SYSDATE) then to_date('01-JAN-1800') else EFFDT end) desc) Q_ORDER
  from CSSTG_OWNER.PS_ACAD_CAR_TBL),
       S as (
select Q1.INSTITUTION_CD, Q1.ACAD_CAR_CD, Q1.SRC_SYS_ID, Q1.EFFDT, Q1.EFF_STAT_CD, 
       Q1.ACAD_CAR_SD, Q1.ACAD_CAR_LD, Q1.ACAD_CAR_CD || ' (' || Q1.ACAD_CAR_LD || ')' ACAD_CAR_CD_DESC,  
       Q1.ACAD_PLAN_TYPE, Q1.FA_CAR_TYPE, Q1.FA_PRIMACY_NBR, Q1.GRADING_BASIS, Q1.GRADING_SCHEME, Q1.GRADE_TRANSFER, Q1.GRADUATE_LVL_IND, Q1.OEE_IND, 
       Q1.REPEAT_CRSE_ERROR, Q1.REPEAT_GRD_CK, Q1.REPEAT_GRD_SUSP, Q1.REPEAT_ENRL_CTL, Q1.REPEAT_ENRL_SUSP, Q1.REPEAT_RULE, Q1.REPEAT_SCHEME, Q1.SF_GRAD_DESIGNATIO,
       DATA_ORIGIN  
  from Q1
 where Q1.Q_ORDER = 1)                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
select nvl(D.ACAD_CAR_SID, ---max(D.ACAD_CAR_SID) over (partition by 1) +  this code does not ignore SID 2147483646 and below line will fix the issue and is added on 2/11/2021
     (select nvl(max(ACAD_CAR_SID),0) from CSMRT_OWNER.PS_D_ACAD_CAR where ACAD_CAR_SID <> 2147483646) +                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
       row_number() over (partition by 1 order by D.ACAD_CAR_SID nulls first)) ACAD_CAR_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
       nvl(D.INSTITUTION_CD, S.INSTITUTION_CD) INSTITUTION_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 
       nvl(D.ACAD_CAR_CD, S.ACAD_CAR_CD) ACAD_CAR_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       nvl(D.SRC_SYS_ID, S.SRC_SYS_ID) SRC_SYS_ID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
       decode(D.EFFDT, S.EFFDT, D.EFFDT, S.EFFDT) EFFDT,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
       decode(D.EFF_STAT_CD, S.EFF_STAT_CD, D.EFF_STAT_CD, S.EFF_STAT_CD) EFF_STAT_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(D.ACAD_CAR_SD, S.ACAD_CAR_SD, D.ACAD_CAR_SD, S.ACAD_CAR_SD) ACAD_CAR_SD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(D.ACAD_CAR_LD, S.ACAD_CAR_LD, D.ACAD_CAR_LD, S.ACAD_CAR_LD) ACAD_CAR_LD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(D.ACAD_CAR_CD_DESC, S.ACAD_CAR_CD_DESC, D.ACAD_CAR_CD_DESC, S.ACAD_CAR_CD_DESC) ACAD_CAR_CD_DESC,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(D.ACAD_PLAN_TYPE, S.ACAD_PLAN_TYPE, D.ACAD_PLAN_TYPE, S.ACAD_PLAN_TYPE) ACAD_PLAN_TYPE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(D.FA_CAR_TYPE, S.FA_CAR_TYPE, D.FA_CAR_TYPE, S.FA_CAR_TYPE) FA_CAR_TYPE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(D.FA_PRIMACY_NBR, S.FA_PRIMACY_NBR, D.FA_PRIMACY_NBR, S.FA_PRIMACY_NBR) FA_PRIMACY_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(D.GRADING_BASIS, S.GRADING_BASIS, D.GRADING_BASIS, S.GRADING_BASIS) GRADING_BASIS,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
       decode(D.GRADING_SCHEME, S.GRADING_SCHEME, D.GRADING_SCHEME, S.GRADING_SCHEME) GRADING_SCHEME,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(D.GRADE_TRANSFER, S.GRADE_TRANSFER, D.GRADE_TRANSFER, S.GRADE_TRANSFER) GRADE_TRANSFER,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(D.GRADUATE_LVL_IND, S.GRADUATE_LVL_IND, D.GRADUATE_LVL_IND, S.GRADUATE_LVL_IND) GRADUATE_LVL_IND,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(D.OEE_IND, S.OEE_IND, D.OEE_IND, S.OEE_IND) OEE_IND,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
       decode(D.REPEAT_CRSE_ERROR, S.REPEAT_CRSE_ERROR, D.REPEAT_CRSE_ERROR, S.REPEAT_CRSE_ERROR) REPEAT_CRSE_ERROR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
       decode(D.REPEAT_GRD_CK, S.REPEAT_GRD_CK, D.REPEAT_GRD_CK, S.REPEAT_GRD_CK) REPEAT_GRD_CK,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
       decode(D.REPEAT_GRD_SUSP, S.REPEAT_GRD_SUSP, D.REPEAT_GRD_SUSP, S.REPEAT_GRD_SUSP) REPEAT_GRD_SUSP,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
       decode(D.REPEAT_ENRL_CTL, S.REPEAT_ENRL_CTL, D.REPEAT_ENRL_CTL, S.REPEAT_ENRL_CTL) REPEAT_ENRL_CTL,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
       decode(D.REPEAT_ENRL_SUSP, S.REPEAT_ENRL_SUSP, D.REPEAT_ENRL_SUSP, S.REPEAT_ENRL_SUSP) REPEAT_ENRL_SUSP,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(D.REPEAT_RULE, S.REPEAT_RULE, D.REPEAT_RULE, S.REPEAT_RULE) REPEAT_RULE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(D.REPEAT_SCHEME, S.REPEAT_SCHEME, D.REPEAT_SCHEME, S.REPEAT_SCHEME) REPEAT_SCHEME,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
       decode(D.SF_GRAD_DESIGNATIO, S.SF_GRAD_DESIGNATIO, D.SF_GRAD_DESIGNATIO, S.SF_GRAD_DESIGNATIO) SF_GRAD_DESIGNATIO,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(D.DATA_ORIGIN, S.DATA_ORIGIN, D.DATA_ORIGIN, S.DATA_ORIGIN) DATA_ORIGIN,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       nvl(D.CREATED_EW_DTTM, SYSDATE) CREATED_EW_DTTM,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       nvl(D.LASTUPD_EW_DTTM, SYSDATE) LASTUPD_EW_DTTM                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
  from S                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
  left outer join CSMRT_OWNER.PS_D_ACAD_CAR D                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
    on D.ACAD_CAR_SID <> 2147483646                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
   and D.INSTITUTION_CD = S.INSTITUTION_CD                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
   and D.ACAD_CAR_CD = S.ACAD_CAR_CD                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
   and D.SRC_SYS_ID = S.SRC_SYS_ID                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
) S                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
    on  (T.INSTITUTION_CD = S.INSTITUTION_CD                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
   and  T.ACAD_CAR_CD = S.ACAD_CAR_CD                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
   and  T.SRC_SYS_ID = S.SRC_SYS_ID)                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
 when matched then update set                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       T.EFFDT = S.EFFDT,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       T.EFF_STAT_CD = S.EFF_STAT_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.ACAD_CAR_SD = S.ACAD_CAR_SD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.ACAD_CAR_LD = S.ACAD_CAR_LD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.ACAD_CAR_CD_DESC = S.ACAD_CAR_CD_DESC,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       T.ACAD_PLAN_TYPE = S.ACAD_PLAN_TYPE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       T.FA_CAR_TYPE = S.FA_CAR_TYPE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.FA_PRIMACY_NBR = S.FA_PRIMACY_NBR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       T.GRADING_BASIS = S.GRADING_BASIS,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       T.GRADING_SCHEME = S.GRADING_SCHEME,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       T.GRADE_TRANSFER = S.GRADE_TRANSFER,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       T.GRADUATE_LVL_IND = S.GRADUATE_LVL_IND,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       T.OEE_IND = S.OEE_IND,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       T.REPEAT_CRSE_ERROR = S.REPEAT_CRSE_ERROR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
       T.REPEAT_GRD_CK = S.REPEAT_GRD_CK,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       T.REPEAT_GRD_SUSP = S.REPEAT_GRD_SUSP,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       T.REPEAT_ENRL_CTL = S.REPEAT_ENRL_CTL,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       T.REPEAT_ENRL_SUSP = S.REPEAT_ENRL_SUSP,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       T.REPEAT_RULE = S.REPEAT_RULE,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.REPEAT_SCHEME = S.REPEAT_SCHEME,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       T.SF_GRAD_DESIGNATIO = S.SF_GRAD_DESIGNATIO,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
       T.DATA_ORIGIN = S.DATA_ORIGIN,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       T.LASTUPD_EW_DTTM = SYSDATE                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
 where                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       decode(T.EFFDT,S.EFFDT,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(T.EFF_STAT_CD,S.EFF_STAT_CD,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(T.ACAD_CAR_SD,S.ACAD_CAR_SD,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(T.ACAD_CAR_LD,S.ACAD_CAR_LD,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(T.ACAD_CAR_CD_DESC,S.ACAD_CAR_CD_DESC,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(T.ACAD_PLAN_TYPE,S.ACAD_PLAN_TYPE,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       decode(T.FA_CAR_TYPE,S.FA_CAR_TYPE,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(T.FA_PRIMACY_NBR,S.FA_PRIMACY_NBR,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       decode(T.GRADING_BASIS,S.GRADING_BASIS,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(T.GRADING_SCHEME,S.GRADING_SCHEME,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       decode(T.GRADE_TRANSFER,S.GRADE_TRANSFER,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       decode(T.GRADUATE_LVL_IND,S.GRADUATE_LVL_IND,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(T.OEE_IND,S.OEE_IND,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       decode(T.REPEAT_CRSE_ERROR,S.REPEAT_CRSE_ERROR,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
       decode(T.REPEAT_GRD_CK,S.REPEAT_GRD_CK,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(T.REPEAT_GRD_SUSP,S.REPEAT_GRD_SUSP,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       decode(T.REPEAT_ENRL_CTL,S.REPEAT_ENRL_CTL,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
       decode(T.REPEAT_ENRL_SUSP,S.REPEAT_ENRL_SUSP,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(T.REPEAT_RULE,S.REPEAT_RULE,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(T.REPEAT_SCHEME,S.REPEAT_SCHEME,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(T.SF_GRAD_DESIGNATIO,S.SF_GRAD_DESIGNATIO,0,1) = 1 or                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
       decode(T.DATA_ORIGIN,S.DATA_ORIGIN,0,1) = 1                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
  when not matched then                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
insert (                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
       T.ACAD_CAR_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       T.INSTITUTION_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
       T.ACAD_CAR_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
