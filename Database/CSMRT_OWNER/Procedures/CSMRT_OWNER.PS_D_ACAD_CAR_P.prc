       T.GRADING_BASIS,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       T.GRADING_SCHEME,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
       T.GRADE_TRANSFER,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
