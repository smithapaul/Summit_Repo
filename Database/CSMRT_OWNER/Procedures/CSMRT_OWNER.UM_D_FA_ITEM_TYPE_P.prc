       Q1.FIN_AID_TYPE, nvl(X4.XLATLONGNAME,'-') FIN_AID_TYPE_LD, 
       Q1.FED_OR_INST, nvl(X5.XLATLONGNAME,'-') FED_OR_INST_LD, 
       Q1.PACKAGE_LIMIT_RULE, Q1.NEED_BASED, Q1.MEET_NEED_COST, Q1.DISBURSE_METHOD, 
       Q1.AGGREGATE_AREA, Q1.FA_ROUND_OPTION, Q1.ROUND_DIRECTION, 
       Q1.REMAINDER_RULE, Q1.FEE_REMAINDER_RULE, Q1.TRUNCATE_FEE, Q1.LOAN_INTEREST_ATTR, 
