        (
                i_TargetTableName   => 'UM_D_FA_ITEM_TYPE',
                i_Action            => 'MERGE',
                i_RowCount          => intRowCount
        );

strMessage01    := 'Updating DATA_ORIGIN on CSMRT_OWNER.UM_D_FA_ITEM_TYPE';
COMMON_OWNER.SMT_LOG.PUT_MESSAGE(i_Message => strMessage01);

strSqlCommand   := 'update DATA_ORIGIN on CSMRT_OWNER.UM_D_FA_ITEM_TYPE';
update CSMRT_OWNER.UM_D_FA_ITEM_TYPE T
   set DATA_ORIGIN = 'D',
       LASTUPD_EW_DTTM = SYSDATE
 where DATA_ORIGIN <> 'D'
   and T.ITEM_TYPE_SID < 2147483646
   and not exists (select 1 
                     from CSSTG_OWNER.PS_ITEM_TYPE_FA S
                    where T.INSTITUTION_CD = S.SETID
                      and T.ITEM_TYPE = S.ITEM_TYPE
                      and T.AID_YEAR = S.AID_YEAR
                      and T.SRC_SYS_ID = S.SRC_SYS_ID)
;

