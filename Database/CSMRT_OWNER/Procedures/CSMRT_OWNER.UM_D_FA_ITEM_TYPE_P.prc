   and Q1.SRC_SYS_ID = X1.SRC_SYS_ID
   and X1.FIELDNAME = 'FA_SOURCE' 
   and X1.X_ORDER = 1  
  left outer join X X2
    on Q1.PRINT_LTR_OPTION = X2.FIELDVALUE
   and Q1.SRC_SYS_ID = X2.SRC_SYS_ID
   and X2.FIELDNAME = 'PRINT_LTR_OPTION' 
   and X2.X_ORDER = 1  
