       Q3 as (
select SETID INSTITUTION_CD, ITEM_TYPE, AID_YEAR, SRC_SYS_ID,  
       UM_DEPT_CNTCT, UM_DONOR_CNTCT, UM_ENDOW_PRJ, UM_DEPARTMENT, DESCR254, 
       1 Q_ORDER
  from CSSTG_OWNER.PS_UM_ITEM_TYPE_FA
