   and Q1.SRC_SYS_ID = X4.SRC_SYS_ID
   and X4.FIELDNAME = 'FIN_AID_TYPE' 
   and X4.X_ORDER = 1  
  left outer join X X5
    on Q1.FED_OR_INST = X5.FIELDVALUE
   and Q1.SRC_SYS_ID = X5.SRC_SYS_ID
   and X5.FIELDNAME = 'FED_OR_INST' 
   and X5.X_ORDER = 1  
  left outer join X X6
    on Q1.PASS_ANTICIP_AID = X6.FIELDVALUE
   and Q1.SRC_SYS_ID = X6.SRC_SYS_ID
   and X6.FIELDNAME = 'PASS_ANTICIP_AID' 
   and X6.X_ORDER = 1  
  left outer join X X7
    on Q1.METHODOLOGY = X7.FIELDVALUE
   and Q1.SRC_SYS_ID = X7.SRC_SYS_ID
   and X7.FIELDNAME = 'METHODOLOGY' 
   and X7.X_ORDER = 1  
 where Q1.Q_ORDER = 1)                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
select nvl(D.ITEM_TYPE_SID, --max(D.ITEM_TYPE_SID) over (partition by 1) +  This code does not ignore SID 2147483646 and below line will fix the issue and is added on 2/12/2021
