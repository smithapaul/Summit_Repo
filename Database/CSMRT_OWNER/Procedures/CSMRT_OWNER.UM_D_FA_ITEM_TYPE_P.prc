  left outer join Q2
    on Q1.INSTITUTION_CD = Q2.INSTITUTION_CD
   and Q1.AID_YEAR = Q2.AID_YEAR
   and Q1.ITEM_TYPE = Q2.ITEM_TYPE
   and Q1.SRC_SYS_ID = Q2.SRC_SYS_ID
