DROP PROCEDURE CSMRT_OWNER.UM_D_CLASS_ASSOC_P
/

--
-- UM_D_CLASS_ASSOC_P  (Procedure) 
--
CREATE OR REPLACE PROCEDURE CSMRT_OWNER."UM_D_CLASS_ASSOC_P" AUTHID CURRENT_USER IS

------------------------------------------------------------------------
-- George Adams
--
-- Loads mart table UM_D_CLASS_ASSOC from PeopleSoft table PS_CLASS_ASSOC.
--
-- V01  SMT-xxxx 02/16/2018,    James Doucette
--                              Converted from DataStage
-- V01.1  Case-42507 07/01/2020,    James Doucette
--                              Fixed CLASS_ASSOC_SID logic.
--
------------------------------------------------------------------------

        strMartId                       Varchar2(50)    := 'CSW';
        strProcessName                  Varchar2(100)   := 'UM_D_CLASS_ASSOC';
        intProcessSid                   Integer;
        dtProcessStart                  Date            := SYSDATE;
        strMessage01                    Varchar2(4000);
        strMessage02                    Varchar2(512);
        strMessage03                    Varchar2(512)   :='';
        strNewLine                      Varchar2(2)     := chr(13) || chr(10);
        strSqlCommand                   Varchar2(32767) :='';
        strSqlDynamic                   Varchar2(32767) :='';
        strClientInfo                   Varchar2(100);
        intRowCount                     Integer;
        intTotalRowCount                Integer         := 0;
        numSqlCode                      Number;
        strSqlErrm                      Varchar2(4000);
        intTries                        Integer;

BEGIN
strSqlCommand := 'DBMS_APPLICATION_INFO.SET_CLIENT_INFO';
DBMS_APPLICATION_INFO.SET_CLIENT_INFO (strProcessName);

strSqlCommand := 'SMT_PROCESS_LOG.PROCESS_INIT';
COMMON_OWNER.SMT_PROCESS_LOG.PROCESS_INIT
        (
                i_MartId                => strMartId,
                i_ProcessName           => strProcessName,
                i_ProcessStartTime      => dtProcessStart,
                o_ProcessSid            => intProcessSid
        );

strMessage01    := 'Merging data into CSMRT_OWNER.UM_D_CLASS_ASSOC';
COMMON_OWNER.SMT_LOG.PUT_MESSAGE(i_Message => strMessage01);

strSqlCommand   := 'merge into CSMRT_OWNER.UM_D_CLASS_ASSOC';
merge /*+ use_hash(S,T) */ into CSMRT_OWNER.UM_D_CLASS_ASSOC T                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
using (                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
  with X as (  
select FIELDNAME, FIELDVALUE, EFFDT, SRC_SYS_ID, 
       XLATLONGNAME, XLATSHORTNAME, DATA_ORIGIN, 
       row_number() over (partition by FIELDNAME, FIELDVALUE, SRC_SYS_ID
                              order by DATA_ORIGIN desc, (case when EFFDT > trunc(SYSDATE) then to_date('01-JAN-1800') else EFFDT end) desc) X_ORDER
  from CSSTG_OWNER.PSXLATITEM
 where DATA_ORIGIN <> 'D'),
       Q1 as (  
select CRSE_ID CRSE_CD, CRSE_OFFER_NBR CRSE_OFFER_NUM, STRM TERM_CD, SESSION_CODE SESSION_CD, ASSOCIATED_CLASS, SRC_SYS_ID, 
       RQRMNT_GROUP, SSR_COMPONENT, COMPONENT_PRIMARY, 
       USE_BLIND_GRADING BLIND_GRADING_FLG, USE_CATLG_RQS CATLOG_RQS_FLG, GRADING_BASIS, GRADE_ROSTER_PRINT,
       INSTRUCTOR_EDIT, RQMNT_DESIGNTN, UPDATE_STDNT_ENRL UPD_STDNT_ENRL_FLG, BILLING_FACTOR, CRSE_COUNT CRSE_CNT, 
       CRSE_CONTACT_HRS, UNITS_MINIMUM MIN_UNITS, UNITS_MAXIMUM MAX_UNITS, UNITS_ACAD_PROG ACAD_PRGRS_UNITS, UNITS_FINAID_PROG FINAID_PRGRS_UNITS, 
       DATA_ORIGIN
  from CSSTG_OWNER.PS_CLASS_ASSOC), 
       S as (
select Q1.CRSE_CD, Q1.CRSE_OFFER_NUM, Q1.TERM_CD, Q1.SESSION_CD, Q1.ASSOCIATED_CLASS, Q1.SRC_SYS_ID,
       nvl(G.RQRMNT_GROUP_SID, 2147483646) RQRMNT_GROUP_SID, nvl(C1.SSR_COMP_SID, 2147483646) SSR_COMP_SID, nvl(C2.SSR_COMP_SID, 2147483646) SSR_COMP_PRIM_SID, 
       Q1.BLIND_GRADING_FLG, Q1.CATLOG_RQS_FLG, 
       Q1.GRADING_BASIS, nvl(X1.XLATSHORTNAME,'-') GRADE_BASIS_SD, nvl(X1.XLATLONGNAME,'-') GRADE_BASIS_LD, 
       Q1.GRADE_ROSTER_PRINT, nvl(X2.XLATSHORTNAME,'-') GRADE_ROSTER_PRINT_SD, nvl(X2.XLATLONGNAME,'-') GRADE_ROSTER_PRINT_LD, 
       Q1.INSTRUCTOR_EDIT, nvl(X3.XLATSHORTNAME,'-') INSTRUCTOR_EDIT_SD, nvl(X3.XLATLONGNAME,'-') INSTRUCTOR_EDIT_LD,  
       Q1.RQMNT_DESIGNTN, Q1.UPD_STDNT_ENRL_FLG, Q1.BILLING_FACTOR, Q1.CRSE_CNT, 
       Q1.CRSE_CONTACT_HRS, Q1.MIN_UNITS, Q1.MAX_UNITS, Q1.ACAD_PRGRS_UNITS, Q1.FINAID_PRGRS_UNITS, 
       Q1.DATA_ORIGIN  
  from Q1
  left outer join CSMRT_OWNER.UM_D_RQ_GRP G   
    on Q1.RQRMNT_GROUP = G.RQRMNT_GROUP 
   and Q1.SRC_SYS_ID = G.SRC_SYS_ID
   and G.DATA_ORIGIN <> 'D'
  left outer join CSMRT_OWNER.PS_D_SSR_COMP C1 
    on Q1.SSR_COMPONENT = C1.SSR_COMP_CD 
   and Q1.SRC_SYS_ID = C1.SRC_SYS_ID
   and C1.DATA_ORIGIN <> 'D'
  left outer join CSMRT_OWNER.PS_D_SSR_COMP C2   
    on Q1.COMPONENT_PRIMARY = C2.SSR_COMP_CD 
   and Q1.SRC_SYS_ID = C2.SRC_SYS_ID
   and C2.DATA_ORIGIN <> 'D'
  left outer join X X1
    on Q1.GRADING_BASIS = X1.FIELDVALUE
   and Q1.SRC_SYS_ID = X1.SRC_SYS_ID
   and X1.FIELDNAME = 'GRADING_BASIS'
   and X1.X_ORDER = 1
  left outer join X X2
    on Q1.GRADE_ROSTER_PRINT = X2.FIELDVALUE
   and Q1.SRC_SYS_ID = X2.SRC_SYS_ID
   and X2.FIELDNAME = 'GRADE_ROSTER_PRINT'
   and X2.X_ORDER = 1
  left outer join X X3
    on Q1.INSTRUCTOR_EDIT = X3.FIELDVALUE
   and Q1.SRC_SYS_ID = X3.SRC_SYS_ID
   and X3.FIELDNAME = 'INSTRUCTOR_EDIT'
   and X3.X_ORDER = 1
    )                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
--select nvl(D.CLASS_ASSOC_SID, max(D.CLASS_ASSOC_SID) over (partition by 1) +                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
--       row_number() over (partition by 1 order by D.CLASS_ASSOC_SID nulls first)) CLASS_ASSOC_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
select nvl(D.CLASS_ASSOC_SID,
(select nvl(max(CLASS_ASSOC_SID),0) from CSMRT_OWNER.UM_D_CLASS_ASSOC where CLASS_ASSOC_SID <> 2147483646) +
row_number() over (partition by 1 order by D.CLASS_ASSOC_SID nulls first)) CLASS_ASSOC_SID, -- June 2020
       nvl(D.CRSE_CD, S.CRSE_CD) CRSE_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       nvl(D.CRSE_OFFER_NUM, S.CRSE_OFFER_NUM) CRSE_OFFER_NUM,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 
       nvl(D.TERM_CD, S.TERM_CD) TERM_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       nvl(D.SESSION_CD, S.SESSION_CD) SESSION_CD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
       nvl(D.ASSOCIATED_CLASS, S.ASSOCIATED_CLASS) ASSOCIATED_CLASS,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
       nvl(D.SRC_SYS_ID, S.SRC_SYS_ID) SRC_SYS_ID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
       decode(D.RQRMNT_GROUP_SID, S.RQRMNT_GROUP_SID, D.RQRMNT_GROUP_SID, S.RQRMNT_GROUP_SID) RQRMNT_GROUP_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(D.SSR_COMP_SID, S.SSR_COMP_SID, D.SSR_COMP_SID, S.SSR_COMP_SID) SSR_COMP_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
       decode(D.SSR_COMP_PRIM_SID, S.SSR_COMP_PRIM_SID, D.SSR_COMP_PRIM_SID, S.SSR_COMP_PRIM_SID) SSR_COMP_PRIM_SID,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
       decode(D.BLIND_GRADING_FLG, S.BLIND_GRADING_FLG, D.BLIND_GRADING_FLG, S.BLIND_GRADING_FLG) BLIND_GRADING_FLG,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
       decode(D.CATLOG_RQS_FLG, S.CATLOG_RQS_FLG, D.CATLOG_RQS_FLG, S.CATLOG_RQS_FLG) CATLOG_RQS_FLG,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(D.GRADING_BASIS, S.GRADING_BASIS, D.GRADING_BASIS, S.GRADING_BASIS) GRADING_BASIS,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
       decode(D.GRADE_BASIS_SD, S.GRADE_BASIS_SD, D.GRADE_BASIS_SD, S.GRADE_BASIS_SD) GRADE_BASIS_SD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(D.GRADE_BASIS_LD, S.GRADE_BASIS_LD, D.GRADE_BASIS_LD, S.GRADE_BASIS_LD) GRADE_BASIS_LD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(D.GRADE_ROSTER_PRINT, S.GRADE_ROSTER_PRINT, D.GRADE_ROSTER_PRINT, S.GRADE_ROSTER_PRINT) GRADE_ROSTER_PRINT,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(D.GRADE_ROSTER_PRINT_SD, S.GRADE_ROSTER_PRINT_SD, D.GRADE_ROSTER_PRINT_SD, S.GRADE_ROSTER_PRINT_SD) GRADE_ROSTER_PRINT_SD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
       decode(D.GRADE_ROSTER_PRINT_LD, S.GRADE_ROSTER_PRINT_LD, D.GRADE_ROSTER_PRINT_LD, S.GRADE_ROSTER_PRINT_LD) GRADE_ROSTER_PRINT_LD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
       decode(D.INSTRUCTOR_EDIT, S.INSTRUCTOR_EDIT, D.INSTRUCTOR_EDIT, S.INSTRUCTOR_EDIT) INSTRUCTOR_EDIT,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
       decode(D.INSTRUCTOR_EDIT_SD, S.INSTRUCTOR_EDIT_SD, D.INSTRUCTOR_EDIT_SD, S.INSTRUCTOR_EDIT_SD) INSTRUCTOR_EDIT_SD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(D.INSTRUCTOR_EDIT_LD, S.INSTRUCTOR_EDIT_LD, D.INSTRUCTOR_EDIT_LD, S.INSTRUCTOR_EDIT_LD) INSTRUCTOR_EDIT_LD,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(D.RQMNT_DESIGNTN, S.RQMNT_DESIGNTN, D.RQMNT_DESIGNTN, S.RQMNT_DESIGNTN) RQMNT_DESIGNTN,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(D.UPD_STDNT_ENRL_FLG, S.UPD_STDNT_ENRL_FLG, D.UPD_STDNT_ENRL_FLG, S.UPD_STDNT_ENRL_FLG) UPD_STDNT_ENRL_FLG,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(D.BILLING_FACTOR, S.BILLING_FACTOR, D.BILLING_FACTOR, S.BILLING_FACTOR) BILLING_FACTOR,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
       decode(D.CRSE_CNT, S.CRSE_CNT, D.CRSE_CNT, S.CRSE_CNT) CRSE_CNT,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       decode(D.CRSE_CONTACT_HRS, S.CRSE_CONTACT_HRS, D.CRSE_CONTACT_HRS, S.CRSE_CONTACT_HRS) CRSE_CONTACT_HRS,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(D.MIN_UNITS, S.MIN_UNITS, D.MIN_UNITS, S.MIN_UNITS) MIN_UNITS,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
       decode(D.MAX_UNITS, S.MAX_UNITS, D.MAX_UNITS, S.MAX_UNITS) MAX_UNITS,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
       decode(D.ACAD_PRGRS_UNITS, S.ACAD_PRGRS_UNITS, D.ACAD_PRGRS_UNITS, S.ACAD_PRGRS_UNITS) ACAD_PRGRS_UNITS,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
       decode(D.FINAID_PRGRS_UNITS, S.FINAID_PRGRS_UNITS, D.FINAID_PRGRS_UNITS, S.FINAID_PRGRS_UNITS) FINAID_PRGRS_UNITS,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                      
       decode(D.DATA_ORIGIN, S.DATA_ORIGIN, D.DATA_ORIGIN, S.DATA_ORIGIN) DATA_ORIGIN,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
       nvl(D.CREATED_EW_DTTM, SYSDATE) CREATED_EW_DTTM,                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        
       nvl(D.LASTUPD_EW_DTTM, SYSDATE) LASTUPD_EW_DTTM                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
  from S                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       
  left outer join CSMRT_OWNER.UM_D_CLASS_ASSOC D                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
    on D.CLASS_ASSOC_SID <> 2147483646                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
   and D.CRSE_CD = S.CRSE_CD                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
   and D.CRSE_OFFER_NUM = S.CRSE_OFFER_NUM                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
   and D.TERM_CD = S.TERM_CD                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                   
   and D.SESSION_CD = S.SESSION_CD                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
   and D.ASSOCIATED_CLASS = S.ASSOCIATED_CLASS                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 
   and D.SRC_SYS_ID = S.SRC_SYS_ID                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
) S                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
    on  (T.CRSE_CD = S.CRSE_CD                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 
   and  T.CRSE_OFFER_NUM = S.CRSE_OFFER_NUM                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
   and  T.TERM_CD = S.TERM_CD                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
